<h1>All Photos</h1>
<% if current_user.nil? %>
	<p>You must <%= link_to "sign up", sign_up_path %> or <%= link_to "login", log_in_path %> to see all photos!</p>
<% else %>
	<h1><div class="button"> <%= link_to "Home", root_path %></div></h1>
	<% for photo in @photos %>
	<li><div>
	<%= file_image_tag photo.image.url(:large) %>
	</div></li>
	<% end %>
<% end %>