<% unless current_user.nil? %>
	<h1>Upload a Photo</h1>

	<%= form_for(@photo, :html => { :multipart => true }) do |f| %>
		<%= f.file_field :image %>
		<%= f.submit "Upload" %>
	<% end %>
	
	<br />
	<p>Want more space on gr8pic? Get a <a href="http://db.tt/6Rmkgg79" target="_blank">dropbox</a> account.</p>
<% else %>
	<%= render "signup_form" %>
<% end %>

